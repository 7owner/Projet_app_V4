<form method="post" action="{{ path('app_rendezvous_delete', {'id': rendezvou.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce rendez-vous ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ rendezvou.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
